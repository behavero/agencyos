{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "crons": [
    {
      "path": "/api/cron/sync-transactions",
      "schedule": "0 */4 * * *"
    },
    {
      "path": "/api/cron/daily-refresh",
      "schedule": "0 1 * * *"
    }
  ]
}
